# Production configuration for SolHunter Zero
HELIUS_API_KEY=replace-with-production-helius-key
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=${HELIUS_API_KEY}
SOLANA_WS_URL=wss://mainnet.helius-rpc.com/?api-key=${HELIUS_API_KEY}
REDIS_URL=redis://prod-redis:6379/0

PAPER_WALLET_KEYPATH=/var/solhunter/wallets/paper.json
LIVE_WALLET_KEYPATH=/var/solhunter/wallets/live.json

DAS_RPS=3
DAS_BURST=6
DAS_DISCOVERY_LIMIT=120
DISCOVERY_OVERFETCH_MULT=2.0
DISCOVERY_RECENT_TTL_SEC=86400

SNAPSHOT_COALESCE_MS=250

MAX_SPREAD_BPS=80
MIN_DEPTH1PCT_USD=8000

VOTE_WINDOW_MS=400
SWARM_QUORUM=2
SWARM_MIN_SCORE=0.04

DEPTH_FRACTION_CAP=0.30
PER_MINT_COOLDOWN_SEC=180
KELLY_CAP=0.15

RL_WEIGHTS_DISABLED=1
