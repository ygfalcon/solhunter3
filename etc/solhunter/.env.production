# Production environment settings for SolHunter.
# Replace each placeholder with the real credential or endpoint before launch.

SOLHUNTER_ENV=production
SOLHUNTER_MODE=live
ENVIRONMENT=production

# --- Core Solana access ---
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=af30888b-b79f-4b12-b3fd-c5375d5bad2d
SOLANA_WS_URL=wss://mainnet.helius-rpc.com/?api-key=af30888b-b79f-4b12-b3fd-c5375d5bad2d
SOLANA_KEYPAIR=/etc/solhunter/id.json
KEYPAIR_PATH=/etc/solhunter/id.json

# --- Helius configuration ---
HELIUS_API_KEY=af30888b-b79f-4b12-b3fd-c5375d5bad2d
HELIUS_API_KEYS=af30888b-b79f-4b12-b3fd-c5375d5bad2d
HELIUS_API_TOKEN=af30888b-b79f-4b12-b3fd-c5375d5bad2d
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=${HELIUS_API_KEY}
HELIUS_WS_URL=wss://mainnet.helius-rpc.com/?api-key=${HELIUS_API_KEY}
HELIUS_PRICE_RPC_URL=https://rpc.helius.xyz
HELIUS_PRICE_REST_URL=https://api.helius.xyz/v0/prices
HELIUS_PRICE_BASE_URL=https://api.helius.xyz
HELIUS_PRICE_TIMEOUT=2.5
HELIUS_PRICE_CONCURRENCY=10

# --- Market data providers ---
BIRDEYE_API_KEY=b1e60d72780940d1bd929b9b2e9225e6
SOLSCAN_API_KEY=
DEX_BASE_URL=https://swap.helius.dev
DEX_TESTNET_URL=https://quote-api.jup.ag
ORCA_API_URL=https://api.orca.so
RAYDIUM_API_URL=https://api.raydium.io
PHOENIX_API_URL=https://app.phoenix.trade
METEORA_API_URL=https://app.meteora.ag
JUPITER_WS_URL=wss://stats.jup.ag/ws

# --- Event bus / persistence ---
REDIS_URL=redis://localhost:6379/0
EVENT_BUS_URL=${REDIS_URL}
BROKER_CHANNEL=solhunter-events
USE_REDIS=1

# --- Feature toggles & risk ---
USE_MEV_BUNDLES=false
USE_DEPTH_STREAM=1
USE_DEPTH_FEED=0
USE_RUST_EXEC=True
USE_SERVICE_EXEC=True
RISK_TOLERANCE=0.15
MAX_ALLOCATION=0.3
RISK_MULTIPLIER=1.0

# --- Observability & runtime ---
UI_PORT=5001
LOG_LEVEL=INFO
RL_HEALTH_HOST=127.0.0.1
RL_HEALTH_PORT=7070

# --- Jito block-engine (required for live bundles) ---
JITO_RPC_URL=https://mainnet.block-engine.jito.wtf/api/v1/bundles
JITO_AUTH=
JITO_WS_URL=wss://mainnet.block-engine.jito.wtf/api/v1/ws
JITO_WS_AUTH=

# --- Analytics / notifications ---
NEWS_FEEDS=https://rss.nytimes.com/services/xml/rss/nyt/Technology.xml,https://www.coindesk.com/arc/outboundfeeds/rss/
TWITTER_FEEDS=
DISCORD_FEEDS=https://discord.com/api/guilds/613425648685547541/widget.json
