# SolHunter Zero environment template
HELIUS_API_KEY=
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=
SOLANA_WS_URL=wss://mainnet.helius-rpc.com/?api-key=
REDIS_URL=redis://localhost:6379/0

# Wallet keypaths (paper/live)
PAPER_WALLET_KEYPATH=~/.config/solana/paper.json
LIVE_WALLET_KEYPATH=~/.config/solana/live.json

# Discovery configuration
DAS_RPS=3
DAS_BURST=6
DAS_DISCOVERY_LIMIT=120
DISCOVERY_OVERFETCH_MULT=2.0
DISCOVERY_RECENT_TTL_SEC=86400

# Snapshot controls
SNAPSHOT_COALESCE_MS=250

# Agent guardrails
MAX_SPREAD_BPS=80
MIN_DEPTH1PCT_USD=8000

# Voting parameters
VOTE_WINDOW_MS=400
SWARM_QUORUM=2
SWARM_MIN_SCORE=0.04

# Execution configuration
DEPTH_FRACTION_CAP=0.30
PER_MINT_COOLDOWN_SEC=180
KELLY_CAP=0.15

# Reinforcement learning defaults
RL_WEIGHTS_DISABLED=1
